<div class="container" style="background-color: white; width: 800px; border: 3px double black; padding: 20px;">
<div class="form-group row">
    <label class="col-form-label col-sm-2" for="Nombre">Nombre</label>
    <div class="col-sm-8">
      <input type="text" autofocus class="form-control" name="Nombre" value="<%= modelo.Nombre %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Apellido">Apellido</label>
    <div class="col-sm-8">
      <input type="text" class="form-control" name="Apellido" value="<%= modelo.Apellido %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="tipoDocumento">Tipo de Documento</label>
    <div class="col-sm-8">
      <input type="text" class="form-control" name="tipoDocumento" value="<%= modelo.tipoDocumento %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="NumeroDocumento">Número de Documento</label>
    <div class="col-sm-8">
      <input type="text" class="form-control" name="NumeroDocumento" value="<%= modelo.NumeroDocumento %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Telefono">Teléfono</label>
    <div class="col-sm-8">
      <input type="text" class="form-control" name="Telefono" value="<%= modelo.Telefono %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Rol">Rol</label>
    <div class="col-sm-8">
      <input type="text" class="form-control" name="Rol" value="<%= modelo.Rol %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Estado">Estado</label>
    <div class="col-sm-8">
      <input type="text" class="form-control" name="Estado" value="<%= modelo.Estado %>" />
    </div>
  </div>