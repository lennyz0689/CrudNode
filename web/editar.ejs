<%- include('header') -%>
<h1>Actualizar Registro</h1>
<form action="/editar/<%=modelo.Id_usuario %>" method="post">
  <div class="form-horizontal">
    <%- include('editorCentral') -%>
    <div class="form-group row">
      <label class="col-form-label col-sm-2"></label>
      <div class="col-sm-10">
        <input type="submit" value="Actualizar" class="btn btn-default btn-success" />
        <a class="btn btn-outline-dark cancel" href="/usuarios">Cancelar</a>
      </div>
    </div>
  </div>
</form>
<%- include('footer') -%>
